{
  "extends": ["standard", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "bracketSpacing": true,
        "semi": false,
        "singleQuote": true,
        "printWidth": 160,
        "trailingComma": "es5"
      }
    ],
    "linebreak-style": ["error", "unix"]
  }
}
